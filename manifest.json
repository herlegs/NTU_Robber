{
  "manifest_version": 2,
  "name": "robber",
  "version": "1.0",
  "description": "robber",
  "browser_action": {
    "default_icon": "rob.png",
    "default_title": "rob it",
    "name": "rob me",
    "default_popup":"popup.html"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "background":{
    "scripts": [
      "background.js"

    ]
  },
  "content_scripts": [
    {
      "matches": ["http://www.ntu.edu.sg/has/SnR/SportsFacilities*"],
      "js": [
        "jquery.js"
      ]
    }
  ]
}
